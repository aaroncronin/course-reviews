<h1 id='title'>ALL THE REVIEWS</h1>
<form id='all_form' method="GET" action="">
    Semester: <select name='semester'>
        <option value="All">All</option>
        <option value="Fall">Fall</option>
        <option value="Spring">Spring</option>
    </select>
    Year: <input style="width: 50px" type="text" name="year">
    Professor: <input  type="text" name="professor">
    <input type="submit" value='Filter'>
</form>
<table>
    <th>Course Number</th>
    <th>Course Name</th>
    <th>Semester</th>
    <th>Year</th>
    <th>Professor</th>
    <th>Review</th>
{{#each reviews}}
<tr>
    <td>{{courseNumber}}</td> 
    <td>{{courseName}}</td>
    <td>{{semester}}</td>
    <td>{{year}}</td>
    <td>{{professor}}</td>
    <td>{{review}}</td>
</tr>
{{/each}}
</table>